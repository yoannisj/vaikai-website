{% set template = 'index' %}

{% extends "layouts/page-landing.nunjucks" %}

{% block head_styles %}
  {{ super() }}
  <link rel="stylesheet" href="{{ 'css/toys.css' }}"></link>
{% endblock %}

{% block body_main %}

  {# include page sections #}
  {% for section in sections %}
    {% include section.template ~ '.nunjucks' %}
  {% endfor %}

{% endblock %}

{% block body_end %}
  {{ super() }}
  <script src="{{ 'js/toys.js' }}"></script>
{% endblock %}